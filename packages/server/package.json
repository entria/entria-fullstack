    "dataloader": "^1.4.0",
    "dotenv-safe": "^4.0.4",
    "fs-extra-promise": "^1.0.1",
    "graphql": "^14.0.0",
    "graphql-playground-middleware-koa": "^1.6.4",
    "graphql-relay": "^0.5.5",
    "graphql-subscriptions": "^0.5.8",
    "jsonwebtoken": "^8.3.0",
    "kcors": "^2.2.2",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-convert": "^1.2.0",
    "koa-graphql": "^0.7.5",
    "koa-graphql-batch": "^1.1.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
